<table class="table">
	<thead class="text-center">
		<tr>
			<th>id</th>
			<th>ユーザー名</th>
			<th>登録日</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td><%= @user.id %></td>
			<td><%= @user.name %></td>
			<td><%= @user.created_at %></td>
		</tr>
	</tbody>
</table>

<table class="table">
	<thead class="text-center">
		<tr>
			<th>タスク名</th>
			<th>作成日</th>
			<th>期限</th>
			<th>ステータス</th>
			<th>タスク詳細</th>
			<th>優先順位</th>
		</tr>
	</thead>
	<tbody>
		<tr>
		<% @tasks.each do |f| %>
			<td><%= f.name %></td>
			<td><%= f.created_at %></td>
			<td><%= f.expired_at %></td>
			<td><%= f.state %></td>
			<td><%= f.description %></td>
			<td><%= f.priority %></td>
		<% end %>
		</tr>
	</tbody>
</table>
<%= link_to 'back', :back %>